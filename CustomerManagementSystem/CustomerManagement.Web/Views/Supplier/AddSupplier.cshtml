
@using CustomerManagement.Web.Models

@model Supplier

@{
    @ViewBag.Title
}

<h1>Add Supplier</h1>

<form method="post">
    <div class="form-group">
        <label asp-for=@Model.SupplierName></label>
        <input class="form-control" asp-for=@Model.SupplierName />
    </div>

    <div class="form-group">
        <label asp-for=@Model.Email></label>
        <input class="form-control" asp-for=@Model.Email />
    </div>

    <input class="btn btn-default" type="submit" name="Save" value="Save" />
</form>
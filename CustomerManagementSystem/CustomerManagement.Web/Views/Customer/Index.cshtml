@model CustomerManagement.Web.ViewModels.CustomerViewModel;

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Action</th>
            </tr>
        </thead>
        
        <tbody>
            @foreach (var customer in Model.Customers)
            {
                @Html.Partial("_Summary", customer)
            }
        </tbody>

    </table>
